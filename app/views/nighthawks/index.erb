<div class="">
  <h1>Welcome home</h1>

  <ul>
    <% @nighthawks.each do |nighthawk| %>
      <li><%= nighthawk.name %> <a href="/nighthawks/<%= nighthawk.id %>">Show</a> <a href="/nighthawks/<%= nighthawk.id%>/edit">Edit</a>
        <form class="" action="/nighthawks/<%= nighthawk.id%>" method="post">
          <input type="hidden" name="_method" value="delete">
          <input type="submit" value="Delete">
        </form>
      </li>
    <% end  %>
  </ul>

  <a href="/nighthawks/new">Create a new Nighthawk</a>
</div>
